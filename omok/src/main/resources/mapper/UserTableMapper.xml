<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.osite.omok.mapper.UserTableMapper">

	<select id="findByUserNum" resultType="UserTable">
		SELECT
			*
		FROM
			user_table
		WHERE
			user_num = #{userNum};
	</select>

</mapper>

	